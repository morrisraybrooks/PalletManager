<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include the database for cloud backup -->
        <include domain="database" path="pallet_manager_database" />
        <include domain="database" path="pallet_manager_database-shm" />
        <include domain="database" path="pallet_manager_database-wal" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Include the database for device transfer -->
        <include domain="database" path="pallet_manager_database" />
        <include domain="database" path="pallet_manager_database-shm" />
        <include domain="database" path="pallet_manager_database-wal" />
    </device-transfer>
</data-extraction-rules>
